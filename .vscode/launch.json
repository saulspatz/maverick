    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python: solver.py",
            "type": "python",
            "request": "launch",
            "program": "${fileDirname}/solver.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["100", "5", "7", "6", "7"],
        },
        {
            "name": "dance",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/src/testDance",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/src",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "maverick test",
